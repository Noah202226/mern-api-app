{
  "version": 2,
  "buildCommand": "concurrently \"npm run build\" \"npm install\" \"node backend/server.js\"",
  "public": true,
  "builds": [
    { "src": "*.html", "use": "@vercel/static" },
    { "src": "*.js", "use": "@vercel/node" }
  ],
  "routes": [{ "src": "/", "dest": "/frontend/dist/index.html" }]
}
